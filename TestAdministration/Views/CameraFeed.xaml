<UserControl
    x:Class="TestAdministration.Views.CameraFeed"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:viewModels="clr-namespace:TestAdministration.ViewModels"
    mc:Ignorable="d"
    d:DataContext="{d:DesignInstance viewModels:CameraFeedViewModel}">

    <StackPanel>
        <Image Source="{Binding CameraFeedImage}" />
        <Border Width="600" />
    </StackPanel>
</UserControl>